# ===========================================
# Genesis Environment Variables Template
# ===========================================
# Copy this to .env for local development
# For Vercel: Dashboard → Settings → Environment Variables
# ===========================================

# SUPABASE (Required for auth & database)
# Get from: Supabase Dashboard → Project Settings → API
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# GOOGLE AUTH (Required for Google One Tap sign-in)
# Get from: Google Cloud Console → APIs & Services → Credentials → OAuth 2.0 Client IDs
# IMPORTANT: Also add this Client ID to Supabase Dashboard:
#   → Authentication → Providers → Google → "Client IDs" field
VITE_GOOGLE_CLIENT_ID=your_google_web_client_id.apps.googleusercontent.com

# PAYMENTS - Paystack (Required for subscriptions)
# Get from: Paystack Dashboard → Settings → API Keys
VITE_PAYSTACK_PUBLIC_KEY=pk_live_your_paystack_public_key
VITE_PAYSTACK_SECRET_KEY=sk_live_your_paystack_secret_key

# AI APIs - Grok (Required for text generation)
# Get from: https://openrouter.ai/keys
VITE_GROK_API_KEY_1=sk-or-v1-your_key_1
VITE_GROK_API_KEY_2=sk-or-v1-your_key_2
VITE_GROK_API_KEY_3=sk-or-v1-your_key_3

# AI APIs - Gemini for Green Room (Required for character conversations)
# Get from: https://makersuite.google.com/app/apikey
# These keys auto-rotate to prevent quota exhaustion
VITE_GREEN_ROOM_API_KEY_1=your_gemini_api_key_1
VITE_GREEN_ROOM_API_KEY_2=your_gemini_api_key_2
VITE_GREEN_ROOM_API_KEY_3=your_gemini_api_key_3
VITE_GREEN_ROOM_API_KEY_4=your_gemini_api_key_4
VITE_GREEN_ROOM_API_KEY_5=your_gemini_api_key_5
VITE_GREEN_ROOM_API_KEY_6=your_gemini_api_key_6
VITE_GREEN_ROOM_API_KEY_7=your_gemini_api_key_7
VITE_GREEN_ROOM_API_KEY_8=your_gemini_api_key_8
VITE_GREEN_ROOM_API_KEY_9=your_gemini_api_key_9
VITE_GREEN_ROOM_API_KEY_10=your_gemini_api_key_10
VITE_GREEN_ROOM_API_KEY_11=your_gemini_api_key_11

# AI APIs - Bytez (Required for image generation)
# Get from: https://bytez.com/dashboard
VITE_BYTEZ_API_KEY=your_bytez_api_key_here

# AI APIs - Bytez Text (Required for text/story generation)
# Separate keys to avoid overcrowding with image generation
# Get from: https://bytez.com/dashboard
VITE_BYTEZ_TEXT_API_KEY=your_bytez_text_api_key_here

# AI APIs - Green Room (Dedicated keys for character conversations)
# Separate keys to isolate Green Room traffic from storybook generation
# Get from: https://bytez.com/dashboard
# Add multiple keys for load balancing (at least 2 recommended)
VITE_GREEN_ROOM_API_KEY_1=your_green_room_api_key_1
VITE_GREEN_ROOM_API_KEY_2=your_green_room_api_key_2
VITE_GREEN_ROOM_API_KEY_3=your_green_room_api_key_3

# OPTIONAL - Debug logging (false in production)
VITE_DEBUG=false

# ===========================================
# ERROR MONITORING - Rollbar
# ===========================================
# Get from: Rollbar Dashboard → Project Settings → Project Access Tokens
# Use the "post_client_item" token (NOT the "write" token)
# 
# PASTE YOUR ROLLBAR ACCESS TOKEN BELOW:
VITE_ROLLBAR_ACCESS_TOKEN=
