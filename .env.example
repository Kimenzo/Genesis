# ===========================================
# Genesis Environment Variables Template
# ===========================================
# Copy this to .env for local development
# For Vercel: Dashboard → Settings → Environment Variables
# ===========================================

# SUPABASE (Required for auth & database)
# Get from: Supabase Dashboard → Project Settings → API
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# GOOGLE AUTH (Required for Google One Tap sign-in)
# Get from: Google Cloud Console → APIs & Services → Credentials → OAuth 2.0 Client IDs
# IMPORTANT: Also add this Client ID to Supabase Dashboard:
#   → Authentication → Providers → Google → "Client IDs" field
VITE_GOOGLE_CLIENT_ID=your_google_web_client_id.apps.googleusercontent.com

# PAYMENTS - Paystack (Required for subscriptions)
# Get from: Paystack Dashboard → Settings → API Keys
VITE_PAYSTACK_PUBLIC_KEY=pk_live_your_paystack_public_key
VITE_PAYSTACK_SECRET_KEY=sk_live_your_paystack_secret_key

# AI APIs - Grok (Required for text generation)
# Get from: https://openrouter.ai/keys
VITE_GROK_API_KEY_1=sk-or-v1-your_key_1
VITE_GROK_API_KEY_2=sk-or-v1-your_key_2
VITE_GROK_API_KEY_3=sk-or-v1-your_key_3

# AI APIs - Gemini (Required for AI generation)
# Get from: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# AI APIs - Bytez (Required for image generation)
# Get from: https://bytez.com/dashboard
VITE_BYTEZ_API_KEY=your_bytez_api_key_here

# OPTIONAL - Debug logging (false in production)
VITE_DEBUG=false
